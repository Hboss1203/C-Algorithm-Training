class Solution {
private:
    vector<int> result;
    void traversal(TreeNode* root){//中序遍历
        //终止条件
        if(root == NULL) return;//到叶子节点了
        //左
        traversal(root->left);
        //中
        result.push_back(root->val);
        //右
        traversal(root->right);
    }
public:
    bool isValidBST(TreeNode* root) {
        result.clear();
        traversal(root);
        for(int i = 1; i < result.size(); i++){
            if(result[i] <= result[i - 1]) return false;// 注意要小于等于，搜索树里不能有相同元素
        }
        return true;
    }
};
